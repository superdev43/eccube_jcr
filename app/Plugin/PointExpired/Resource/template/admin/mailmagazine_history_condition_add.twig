<table style="display:none;">
<tbody id="point_expired">
    <tr>
        <th class="w-25">{{ 'pointexpired.admin.customer.label.point_expired'|trans }}</th>
        <td>
            {% if search_data.point_expired_date_start is defined %}
                {% if search_data.point_expired_date_start is null %}
                    ({{ 'mailmagazine.history.condition.label_null'|trans }})
                {% else %}
                    {{ search_data.point_expired_date_start|date("Y/m/d") }}
                {% endif %}
                ï½ž
                {% if search_data.point_expired_date_end is null %}
                    ({{ 'mailmagazine.history.condition.label_null'|trans }})
                {% else %}
                    {{ search_data.point_expired_date_end|date("Y/m/d") }}
                {% endif %}
            {% endif %}
        </td>
    </tr>
</tbody>
</table>

<script>
    $(function() {
        $elem = $('#point_expired');
        $('tbody:eq(0)').append($elem.html());
        $elem.remove();
    });
</script>